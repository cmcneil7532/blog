<%- include('partials/header') %>

<h1>Home</h1>

<p><%= home %></p>

<% postArray.forEach((element)=>{ %>
<%let link = element.title.toLowerCase().split(" ").join('-') %>
<h1><%= element.title %></h1>
<p style="display: inline-block;"><%= element.post.substring(0, 100) %>...</p> 
<a href="/posts/<%= link %>">Read More</a>
<% }) %> <%- include('partials/footer') %>
